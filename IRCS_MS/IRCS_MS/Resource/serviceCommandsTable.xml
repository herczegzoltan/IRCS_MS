    <Record name = "FunctionGen_Off"  value = "0x08"/>
    <Record name = "Analyser_On"  value = "0x09"/>
    <Record name = "Analyser_Off"  value = "0x0a"/>
    <Record name = "PsuOn"  value = "0x0b"/>
